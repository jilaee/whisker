{
    "name": "whisker",
    "version": "0.4.0",
    "description": "A Testing Utility for Scratch 3.0",
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/se2p/whisker-main"
    },
    "private": true,
    "workspaces": [
        "whisker-main",
        "whisker-web",
        "servant"
    ],
    "scripts": {
        "build": "yarn --cwd whisker-main build && yarn --cwd whisker-web build",
        "build-main": "yarn --cwd whisker-main build",
        "build-web": "yarn --cwd whisker-web build",
        "servant": "yarn --cwd servant node servant.js",
        "test": "yarn --cwd whisker-main test",
        "integrationtest": "yarn --cwd whisker-web test"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.12.7",
        "@types/jest": "^26.0.18",
        "@types/node": "^14.14.11",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-preset-minify": "^0.5.1",
        "copy-webpack-plugin": "^6.3.2",
        "css-loader": "^5.0.1",
        "eslint": "^7.15.0",
        "eslint-config-scratch": "^6.0.0",
        "expose-loader": "^1.0.3",
        "file-url": "3.0.0",
        "imports-loader": "^1.2.0",
        "ink-docstrap": "^1.3.2",
        "jest": "^26.6.3",
        "jest-puppeteer": "4.4.0",
        "jsdoc": "^3.6.6",
        "mini-css-extract-plugin": "^1.3.2",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "tap": "^14.11.0",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.11",
        "ts-node": "^9.1.0",
        "typescript": "^4.1.2",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12"
    }
}
