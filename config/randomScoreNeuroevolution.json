{
    "test-generator": "neuroevolution",
    "algorithm": "randomNeuroevolution",
    "chromosome": "neatChromosome",
    "population-size": 100,
    "parentsPerSpecies": 0.20,
    "penalizingAge": 20,
    "ageSignificance": 1.0,
    "inputRate": 0.3,
    "crossover": {
        "operator": "neatCrossover",
        "crossoverWithoutMutation": 0.2,
        "interspeciesRate": 0.001,
        "weightAverageRate": 0.4
    },
    "mutation": {
        "operator": "neatMutation",
        "mutationWithoutCrossover": 0.25,
        "mutationAddConnection": 0.05,
        "recurrentConnection": 0.05,
        "addConnectionTries": 20,
        "populationChampionConnectionMutation": 0.3,
        "mutationAddNode": 0.03,
        "mutateWeights": 0.6,
        "perturbationPower": 2.0,
        "mutateToggleEnableConnection": 0.1,
        "toggleEnableConnectionTimes": 3,
        "mutateEnableConnection": 0.03
    },
    "compatibility": {
        "distanceThreshold": 3.0,
        "disjointCoefficient": 1,
        "excessCoefficient": 1,
        "weightCoefficient": 0.4
    },
    "fitness-function": {
        "type": "statement",
        "targets": []
    },
    "network-fitness": {
        "type": "combined",
        "timeout": 20000,
        "functions": [
            {
                "type": "score",
                "offset": 0.01
            },
            {
                "type": "statement"
            }
        ]
    },
    "stopping-condition": {
        "type": "one-of",
        "conditions": [
            {
                "type": "fixed-iteration",
                "iterations": 200
            },
            {
                "type": "optimal"
            }
        ]
    },
    "waitStepUpperBound": 200,
    "click-duration": 1,
    "press-duration": 1
}
