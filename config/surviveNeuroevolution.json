        "mutateToggleEnableConnection": 0.1,
        "toggleEnableConnectionTimes": 3,
        "mutateEnableConnection": 0.03
    },
    "compatibility": {
        "distanceThreshold": 5.0,
        "disjointCoefficient": 1,
        "excessCoefficient": 1,
        "weightCoefficient": 0.4
    },
    "fitness-function": {
        "type": "statement",
        "targets": []
