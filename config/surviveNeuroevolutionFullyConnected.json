{
    "test-generator": "neuroevolution",
    "algorithm": "neat",
    "chromosome": "fullyConnectedNetwork",
    "population-size": 500,
    "parentsPerSpecies": 0.20,
    "penalizingAge": 15,
    "ageSignificance": 1.0,
    "inputRate": 0.3,
    "crossover": {
        "operator": "neatCrossover",
        "crossoverWithoutMutation": 0.2,
        "interspeciesRate": 0.001,
        "weightAverageRate": 0.4
    },
    "mutation": {
        "operator": "neatMutation",
        "mutationWithoutCrossover": 0.25,
        "mutationAddConnection": 0.1,
        "recurrentConnection": 0.1,
        "addConnectionTries": 20,
        "populationChampionNumberOffspring": 10,
        "populationChampionNumberClones": 5,
        "populationChampionConnectionMutation": 0.3,
        "mutationAddNode": 0.03,
        "mutateWeights": 0.8,
        "perturbationPower": 2.5,
        "mutateToggleEnableConnection": 0.1,
        "toggleEnableConnectionTimes": 3,
        "mutateEnableConnection": 0.03
    },
    "compatibility": {
        "distanceThreshold": 3.0,
        "disjointCoefficient": 1,
        "excessCoefficient": 1,
        "weightCoefficient": 1
    },
    "fitness-function": {
        "type": "statement",
        "targets": []
    },
    "network-fitness": {
        "type": "survive",
        "timeout": 1000000
    },
    "stopping-condition": {
        "type": "fixed-iteration",
        "iterations": 300
    },
    "waitStepUpperBound": 200,
    "click-duration": 1,
    "press-duration": 1
}
